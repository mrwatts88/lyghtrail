{
  "name": "lyghtrail-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "EXPO_PUBLIC_STAGE='staging' expo start",
    "dev:build:mobile:sim": "EXPO_PUBLIC_STAGE='staging' eas build --platform ios --profile dev-sim",
    "dev:build:mobile:device": "EXPO_PUBLIC_STAGE='staging' eas build --platform ios --profile dev-device",
    "build:web:prod": "EXPO_PUBLIC_STAGE='production' expo export --platform web --output-dir ../server/dist",
    "build:mobile:staging": "EXPO_PUBLIC_STAGE='staging' eas build --platform all --profile staging",
    "build:mobile:prod": "EXPO_PUBLIC_STAGE='production' eas build --platform all --profile production"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.20.6",
    "@clerk/clerk-react": "^4.30.6",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.0.2",
    "expo": "~50.0.7",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.11",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "swr": "^2.2.5",
    "expo-dev-client": "~3.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.2",
    "nodemon": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
